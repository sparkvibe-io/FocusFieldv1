<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Distribution method for App Store -->
	<key>method</key>
	<string>app-store</string>

	<!-- Upload symbols for crash reporting -->
	<key>uploadSymbols</key>
	<true/>

	<!-- Upload bitcode (optional, Apple is phasing this out) -->
	<key>uploadBitcode</key>
	<false/>

	<!-- Compile bitcode (optional) -->
	<key>compileBitcode</key>
	<false/>

	<!-- Team ID - Your Apple Developer Team ID -->
	<!-- Found in Xcode project configuration -->
	<key>teamID</key>
	<string>2R2QXHHW39</string>

	<!-- Provisioning profiles for each bundle identifier -->
	<!-- The key is the bundle ID, the value is the provisioning profile name -->
	<key>provisioningProfiles</key>
	<dict>
		<key>io.sparkvibe.focusfield</key>
		<string>FocusFieldDistributionProfile</string>
	</dict>

	<!-- Code signing style: manual (we're using specific profile) -->
	<key>signingStyle</key>
	<string>manual</string>

	<!-- Code signing certificate: Apple Distribution -->
	<key>signingCertificate</key>
	<string>Apple Distribution</string>

	<!-- Manage app version and build number -->
	<key>manageAppVersionAndBuildNumber</key>
	<false/>

	<!-- Destination: export only (don't upload) -->
	<key>destination</key>
	<string>export</string>
</dict>
</plist>
